{
  "manifest_version": 2,

  "name": "TabCapture",
  "description": "Saves all open browsers ",
  "version": "1.0",

  "icons": { 
    "16":"images/icon_16.png", 
    "48":"images/icon_48.png", 
    "128":"images/icon_128.png" 
  }, 

  "browser_action": {
    "default_icon": "images/icon_48.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "storage"
  ]
}